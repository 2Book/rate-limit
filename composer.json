{
    "name": "nikolaposa/rate-limit",
    "description": "General purpose rate limiter implementation. Also features PSR-15 middleware suitable for APIs.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "rate limit",
        "redis",
        "psr-15",
        "middleware"
    ],
    "authors": [
        {
            "name": "Nikola Po≈°a",
            "email": "posa.nikola@gmail.com",
            "homepage": "https://www.nikolaposa.in.rs"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "beberlei/assert": "^3.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.1",
        "laminas/laminas-diactoros": "^2.2",
        "phpstan/phpstan": "^0.12.10",
        "phpstan/phpstan-beberlei-assert": "^0.12.2",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpunit/phpunit": "^8.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "suggest": {
        "laminas/laminas-diactoros": "In order to use RateLimitMiddleware",
        "psr/http-message": "In order to use RateLimitMiddleware",
        "psr/http-server-handler": "In order to use RateLimitMiddleware"
    },
    "autoload": {
        "psr-4": {
            "RateLimit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RateLimit\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "cs-fix": "php-cs-fixer fix --config=.php_cs",
        "stan": "phpstan analyse"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
